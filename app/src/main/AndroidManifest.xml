<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hardikdesaii.contentresolverdemo">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.READ_CALL_LOG" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <application
        android:name=".AppClass"
        android:allowBackup="true"
        android:icon="@mipmap/backup1"
        android:label=""
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="HOME"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".FetchContacts"
            android:label="CONTACT"/>
        <activity android:name=".FetchSms"
            android:label="SMS"/>
        <activity android:name=".DemoActivity"></activity>
        <activity android:name=".Login"
            android:label="">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Signup"
            android:label="SIGNUP"></activity>
    </application>

</manifest>